---
layout: null
---
{
  {% assign audio_data = site.data.audio %}
  {% for item_hash in audio_data %}
    {% assign object = item_hash[1] %}
    {% assign id = item_hash[0] %}
    "audio/{{id}}": {
      "href": "audio/{{id}}",
      "title": "{{object.title | escape | strip_newlines}} ({% for instance in object.instances %}{% if instance.instance_type == 'Moving Images' %}{{ instance.container.indicator_1 }}{% elsif instance.instance_type == 'audio' %}{{ instance.container.indicator_1 }}{% endif %}{% endfor %})",
      "url": "{{site.url}}/audio/{{id}}",
      "record_type": "audio"
    },
  {% endfor %}
  {% assign moving-image_data = site.data.moving-image %}
  {% for item_hash in moving-image_data %}
    {% assign object = item_hash[1] %}
    {% assign id = item_hash[0] %}
    "moving-image/{{id}}": {
      "href": "moving-image/{{id}}",
      "title": "{{object.title | escape | strip_newlines}} ({% for instance in object.instances %}{% if instance.instance_type == 'Moving Images' %}{{ instance.container.indicator_1 }}{% elsif instance.instance_type == 'audio' %}{{ instance.container.indicator_1 }}{% endif %}{% endfor %})",
      "url": "{{site.url}}/moving-image/{{id}}",
      "record_type": "moving-image"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %},
  {% assign catalogued-reports_data = site.data.catalogued-reports %}
  {% for item_hash in catalogued-reports_data %}
    {% assign object = item_hash[1] %}
    {% assign id = item_hash[0] %}
    "catalogued-reports/{{id}}": {
      "href": "catalogued-reports/{{id}}",
      "title": "{{object.title | escape | strip_newlines}}",
      "url": "{{site.url}}/catalogued-reports/{{id}}",
      "record_type": "catalogued-reports"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
}
